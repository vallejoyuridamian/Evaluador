// Debug
type
  Logger = class
    public
      procedure logInteger(nombre: String; valor: Integer);
      procedure logString(nombre: String; valor: String);
  end;

implementation
uses
  Classes, SysUtils;

procedure Logger.logInteger(nombre: string; valor: Integer);
begin
  with TStringList.Create do
    try
      Add(nombre + ' vale = ' + IntToStr(valor));
      SaveToFile('pruebaDebug.txt');
    finally
      Free;
    end;
end;

procedure Logger.logString(nombre: String; valor: String);
begin
  with TStringList.Create do
    try
      Add(nombre + ' vale = ' + valor);
      SaveToFile('pruebaDebug.txt');
    finally
      Free;
    end;
